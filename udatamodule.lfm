  object zqTransactions: TZQuery
    Connection = zcDatabaseConnection
    AfterOpen = zqTransactionsAfterOpen
    SQL.Strings = (
      'select transid, a.accountid, a.accountname, strftime(''%Y/%m/%d'',transdate) frmtrandate, transdate,'
      'transactionnumber, payeename, ch.status, '
      'case ch.categid when -1 then '''''
      'else'
      '  case ch.subcategid when -1 then categname '
      '  else coalesce(categname,'''') ||'': ''||coalesce(subcategname,'''') '
      '  end '
      'end as category, '
      'case transcode '
      'when ''Withdrawal'' then '
      'case when substr(transamount,length(ROUND(transamount,2))-1,1) = "." then transamount || "0" '
      'when substr(transamount,length(ROUND(transamount,2))-1,1) = "" then transamount || ".00" '
      'else transamount '
      'end '
      'else '''''
      'end as WITHDRAWAL, '
      'case transcode '
      'when ''Deposit'' then '
      'case when substr(transamount,length(ROUND(transamount,2))-1,1) = "." then transamount || "0" '
      'when substr(transamount,length(ROUND(transamount,2))-1,1) = "" then transamount || ".00" '
      'else transamount '
      'end '
      'else '''''
      'end as DEPOSIT, '
      'amount, '
      ' case when substr((select round(sum(case transcode when ''Withdrawal'' then transamount * -1 '
      'else transamount end), 2)  '
      ' from CHECKINGACCOUNT_V1 where accountid = ch.accountid and transid<=ch.transid),length(ROUND((select round(sum(case transcode when ''Withdrawal'' then transamount * -1 '
      'else transamount end), 2)  '
      ' from CHECKINGACCOUNT_V1 where accountid = ch.accountid and transid<=ch.transid),2))-1,1) = "." '
      ' then (select round(sum(case transcode when ''Withdrawal'' then transamount * -1 '
      'else transamount end), 2)  '
      ' from CHECKINGACCOUNT_V1 where accountid = ch.accountid and transid<=ch.transid) || "0"  when substr((select round(sum(case transcode when ''Withdrawal'' then transamount * -1 '
      'else transamount end), 2)  '
      ' from CHECKINGACCOUNT_V1 where accountid = ch.accountid and transid<=ch.transid),length(ROUND((select round(sum(case transcode when ''Withdrawal'' then transamount * -1 '
      'else transamount end), 2)  '
      ' from CHECKINGACCOUNT_V1 where accountid = ch.accountid and transid<=ch.transid),2))-1,1) = "" then (select round(sum(case transcode when ''Withdrawal'' then transamount * -1 '
      'else transamount end), 2)  '
      ' from CHECKINGACCOUNT_V1 where accountid = ch.accountid and transid<=ch.transid) || ".00"  else (select round(sum(case transcode when ''Withdrawal'' then transamount * -1 '
      'else transamount end), 2)  '
      ' from CHECKINGACCOUNT_V1 where accountid = ch.accountid and transid<=ch.transid)  '
      ' end as BALANCE,'
      'transcode,transamount, ch.notes'
      'from checkingaccount_v1 ch'
      'inner join payee_v1 p on ch.payeeid = p.payeeid'
      'inner join accountlist_v1 a on ch.accountid = a.accountid'
      'left outer join category_v1 c on ch.categid = c.categid '
      'left outer join subcategory_v1 sc on c.categid = sc.categid and ch.subcategid = sc.subcategid '
      'where 1 = 1'
      'order by transid, transdate'
    )
