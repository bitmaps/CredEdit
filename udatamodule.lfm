  object zqAssets: TZQuery
    Connection = zcDatabaseConnection
    AfterOpen = zqAssetsAfterOpen
    AfterRefresh = zqAssetsAfterRefresh
    OnCalcFields = zqAssetsCalcFields
    SQL.Strings = (
      'select assetid, assetname, startdate, value, '
      'valuechange, valuechangerate,  notes, assettype, '
      ' strftime(''%Y/%m/%d'',startdate)  frmstartdate,'
      'case when valuechange = ''Appreciates'' then'
      'value + (cast(round(julianday(''now'') - julianday(startdate)) as integer) / 365) * '
      '((value / 100) * valuechangerate) '
      'when valuechange = ''Depreciates'' then'
      'value - (cast(round(julianday(''now'') - julianday(startdate)) as integer) / 365) * '
      '((value / 100) * valuechangerate) '
      'else value'
      'end as CURRENTVALUE'
      'from ASSETS order by assetid'
    )
