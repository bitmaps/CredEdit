  object zqBankAccounts: TZQuery
    Connection = zcDatabaseConnection
    AfterOpen = zqBankAccountsAfterOpen
    AfterRefresh = zqBankAccountsAfterRefresh
    OnCalcFields = zqBankAccountsCalcFields
    SQL.Strings = (
      ' select ch.transid,  strftime(''%Y/%m/%d'',transdate)  frmtrandate,  ch.transactionnumber, ch.status, ch.payeeid,  '
      ' '
      ' case when (ch.payeeid = -1) then  case when (ch.accountid = 2) then ''> ''||ta.accountname else ''< ''||ta.accountname  '
      ' end else p.payeename  end as PAYEE,  '
      ' p.payeename,  '
      ' '
      ' case s.transid when ch.transid then ''...'' else  '
      ' case ch.categid when 0 then '''' else  '
      ' case ch.categid when -1 then '''' else    '
      ' case ch.subcategid when -1 then categname    else coalesce(categname,'''') ||'': ''||coalesce(subcategname,'''')    '
      ' end  end end end as category,  '
      ' '
      ' case when (transcode = ''Withdrawal'' or transcode = ''Transfer'') then  '
      ' case when (transcode = ''Transfer'' and ch.accountid = 2) then  '
      ' case when substr(transamount,length(ROUND(transamount,2))-1,1) = "." then transamount || "0"  '
      ' when substr(transamount,length(ROUND(transamount,2))-1,1) = "" then transamount || ".00"  '
      ' else transamount end else  case when (transcode = ''Withdrawal'') then  '
      ' case when substr(transamount,length(ROUND(transamount,2))-1,1) = "." then transamount || "0"  '
      ' when substr(transamount,length(ROUND(transamount,2))-1,1) = "" then transamount || ".00"  '
      ' else transamount end else ''''  end end end as WITHDRAWAL,  '
      ' '
      ' case when (transcode = ''Deposit'' or transcode = ''Transfer'') then  '
      ' case when (transcode = ''Transfer'' and ch.toaccountid =2) then  '
      ' case when substr(transamount,length(ROUND(transamount,2))-1,1) = "." then transamount || "0"  '
      ' when substr(transamount,length(ROUND(transamount,2))-1,1) = "" then transamount || ".00"  '
      ' else transamount end else  case when (transcode = ''Deposit'') then  '
      ' case when substr(transamount,length(ROUND(transamount,2))-1,1) = "." then transamount || "0"  '
      ' when substr(transamount,length(ROUND(transamount,2))-1,1) = "" then transamount || ".00"  '
      ' else transamount end else ''''   end end end as DEPOSIT,  '
      ' '
      ' case when substr((select round(sum(case transcode when ''Withdrawal'' then transamount * -1  else transamount end), 2)  '
      ' from CHECKINGACCOUNT_V1 where accountid = ch.accountid and transid<=ch.transid),'
      ' length(ROUND((select round(sum(case transcode when ''Withdrawal'' then transamount * -1  else transamount end), 2)  '
      ' from CHECKINGACCOUNT_V1 where accountid = ch.accountid and transid<=ch.transid),2))-1,1) = "."  '
      ' then (select round(sum(case transcode when ''Withdrawal'' then transamount * -1  else transamount end), 2)  '
      ' from CHECKINGACCOUNT_V1 where accountid = ch.accountid and transid<=ch.transid) || "0"  '
      ' when substr((select round(sum(case transcode when ''Withdrawal'' then transamount * -1  else transamount end), 2)  '
      ' from CHECKINGACCOUNT_V1 where accountid = ch.accountid and transid<=ch.transid),'
      ' length(ROUND((select round(sum(case transcode when ''Withdrawal'' then transamount * -1  else transamount end), 2)  '
      ' from CHECKINGACCOUNT_V1 where accountid = ch.accountid and transid<=ch.transid),2))-1,1) = "" '
      ' then (select round(sum(case transcode when ''Withdrawal'' then transamount * -1  else transamount end), 2)  '
      ' from CHECKINGACCOUNT_V1 where accountid = ch.accountid and transid<=ch.transid) || ".00"  '
      ' else (select round(sum(case transcode when ''Withdrawal'' then transamount * -1  else transamount end), 2)  '
      ' from CHECKINGACCOUNT_V1 where accountid = ch.accountid and transid<=ch.transid)  '
      ' end as BALANCE,  '
      ' '
      ' ch.transcode,ch.transamount, ch.notes, ch.transdate, ch.subcategid, ch.categid  '
      ' from CHECKINGACCOUNT_V1 ch '
      ' left outer join PAYEE_V1 p on ch.payeeid = p.payeeid  '
      ' left outer join ACCOUNTLIST_V1 ta on ch.toaccountid = ta.accountid  '
      ' left outer join CATEGORY_V1 c on ch.categid = c.categid  '
      ' left outer join SUBCATEGORY_V1 sc on c.categid = sc.categid and ch.subcategid = sc.subcategid  '
      ' left outer join SPLITTRANSACTIONS_V1 s on ch.transid = s.transid  '
      ' where (ch.accountid = 2 or ch.toaccountid = 2)  '
      ' group by ch.transid '
    )
