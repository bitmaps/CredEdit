      '-- tf.fromdate FRMFROMDATE, tf.todate FRMTODATE,'
      'categname ||'': ''||subcategname CATEGORY'
      'from transfilters tf'
      'left outer join category_v1 c on tf.categid = c.categid '
      'left outer join subcategory_v1 sc on c.categid = sc.categid and tf.subcategid = sc.subcategid'
